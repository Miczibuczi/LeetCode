   "source": [
    "class Solution:\n",
    "    def countBits(self, n: int) -> List[int]:\n",
    "        solution = []\n",
    "        for i in range(n+1):\n",
    "            count = 0\n",
    "            for j in bin(i):\n",
    "                if j == \"1\":\n",
    "                    count += 1\n",
    "            solution.append(count)\n",
    "        return solution"
   ]
  }
