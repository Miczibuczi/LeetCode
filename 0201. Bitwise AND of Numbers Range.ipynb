   "source": [
    "class Solution:\n",
    "    def rangeBitwiseAnd(self, left: int, right: int) -> int:\n",
    "        ans = right\n",
    "        while ans > left:\n",
    "            ans = ans & (ans-1)\n",
    "        return ans"
   ]
  }
