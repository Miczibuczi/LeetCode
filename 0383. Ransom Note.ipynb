   "source": [
    "class Solution:\n",
    "    def canConstruct(self, ransomNote: str, magazine: str) -> bool:\n",
    "        for i in ransomNote:\n",
    "            if i not in magazine:\n",
    "                return False\n",
    "            else:\n",
    "                magazine = magazine.replace(i, \"\", 1)\n",
    "        return True"
   ]
  }
