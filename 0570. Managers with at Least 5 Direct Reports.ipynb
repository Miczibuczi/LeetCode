   "source": [
    "import pandas as pd\n",
    "\n",
    "def find_managers(employee: pd.DataFrame) -> pd.DataFrame:\n",
    "    ans = employee.groupby(\"managerId\").size().reset_index(name=\"count\")\n",
    "    ans = ans[ans[\"count\"]>=5]\n",
    "    employee = employee[employee[\"id\"].isin(ans[\"managerId\"])]\n",
    "    return employee[[\"name\"]]"
   ]
  }
