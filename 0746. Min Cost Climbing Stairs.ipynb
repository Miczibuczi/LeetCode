   "source": [
    "class Solution:\n",
    "    def minCostClimbingStairs(self, cost: List[int]) -> int:\n",
    "        cumulated_cost = [0, 0]\n",
    "        for i in range(2, len(cost)+1):\n",
    "            cumulated_cost.append(min(cumulated_cost[i-2]+cost[i-2], cumulated_cost[i-1]+cost[i-1]))\n",
    "        return cumulated_cost[-1]"
   ]
  }
