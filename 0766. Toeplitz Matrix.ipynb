    "class Solution:\n",
    "    def isToeplitzMatrix(self, matrix: List[List[int]]) -> bool:\n",
    "        rows = len(matrix)\n",
    "        columns = len(matrix[0])\n",
    "        for i in range(rows-1):\n",
    "            if matrix[i][:-1] != matrix[i+1][1:]:\n",
    "                return False\n",
    "        return True"
   ]
