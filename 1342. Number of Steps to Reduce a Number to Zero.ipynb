   "source": [
    "class Solution:\n",
    "    def numberOfSteps(self, num: int) -> int:\n",
    "        num = str(bin(num))[2:]\n",
    "        zeroes = num.count(\"0\")\n",
    "        ones = num.count(\"1\")\n",
    "        return ones*2 + zeroes*1 -1"
   ]
  }
