   "source": [
    "class Solution:\n",
    "    def isAnagram(self, s: str, t: str) -> bool:\n",
    "        for i in s:\n",
    "            if i not in t:\n",
    "                return False\n",
    "            else:\n",
    "                t=t.replace(i,\"\",1)\n",
    "        return len(t) == 0"
   ]
  }
