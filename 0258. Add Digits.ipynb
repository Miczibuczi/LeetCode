   "source": [
    "class Solution:\n",
    "    def addDigits(self, num: int) -> int:\n",
    "        while num >= 10:\n",
    "            new_num = 0\n",
    "            while num > 0:\n",
    "                new_num += num % 10\n",
    "                num = num // 10\n",
    "            num = new_num\n",
    "        return num"
   ]
  }
