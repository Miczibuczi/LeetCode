   "source": [
    "class Solution:\n",
    "    def repeatedNTimes(self, nums: List[int]) -> int:\n",
    "        i = 2\n",
    "        while True:\n",
    "            if len(set(nums[:i])) != i:\n",
    "                return nums[i-1]\n",
    "            i += 1"
   ]
