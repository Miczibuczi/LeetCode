   "source": [
    "class Solution:\n",
    "    def uniquePaths(self, m: int, n: int) -> int:\n",
    "        board = [[1 for i in range(n)]for j in range(m)]\n",
    "        for i in range(1, m):\n",
    "            for j in range(1, n):\n",
    "                board[i][j] = board[i-1][j]+board[i][j-1]\n",
    "        return board[-1][-1]"
   ]
  }
