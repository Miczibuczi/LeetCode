   "source": [
    "class Solution:\n",
    "    def isUgly(self, n: int) -> bool:\n",
    "        if n == 0:\n",
    "            return False\n",
    "        numbers = [2, 3, 5]\n",
    "        for i in numbers:\n",
    "            while n % i == 0:\n",
    "                n = n/i\n",
    "        return n == 1"
   ]
  }
