   "source": [
    "class Solution:\n",
    "    def isPowerOfTwo(self, n: int) -> bool:\n",
    "        if n < 1:\n",
    "            return False\n",
    "            \n",
    "        while n > 1:\n",
    "            if n % 2 == 1:\n",
    "                return False\n",
    "            else:\n",
    "                n = n/2\n",
    "        return True"
   ]
  }
