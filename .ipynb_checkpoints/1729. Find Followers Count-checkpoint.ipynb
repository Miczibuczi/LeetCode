   "source": [
    "import pandas as pd\n",
    "\n",
    "def count_followers(followers: pd.DataFrame) -> pd.DataFrame:\n",
    "    return followers.groupby(by=\"user_id\").agg(followers_count=(\"follower_id\", \"count\")).reset_index()"
   ]
  }
