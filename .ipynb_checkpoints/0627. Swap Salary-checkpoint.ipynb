   "source": [
    "import pandas as pd\n",
    "\n",
    "def swap_salary(salary: pd.DataFrame) -> pd.DataFrame:\n",
    "    salary[\"sex\"] = salary[\"sex\"].apply(lambda x: \"m\" if x == \"f\" else \"f\")\n",
    "    return salary"
   ]
  }
