   "source": [
    "import pandas as pd\n",
    "\n",
    "def find_classes(courses: pd.DataFrame) -> pd.DataFrame:\n",
    "    count = courses.value_counts(\"class\")\n",
    "    ans = []\n",
    "    for key, value in count.items():\n",
    "        if value >= 5:\n",
    "            ans.append(key)\n",
    "    return pd.DataFrame({\"class\":ans})"
   ]
  }
