   "source": [
    "class Solution:\n",
    "    def maximumGap(self, nums: List[int]) -> int:\n",
    "        if len(nums) == 1:\n",
    "            return 0\n",
    "        nums.sort()\n",
    "        max_diff = 0\n",
    "        for i, num in enumerate(nums[1:]):\n",
    "            max_diff = max(max_diff, num-nums[i])\n",
    "        return max_diff"
   ]
  }
