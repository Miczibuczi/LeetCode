   "source": [
    "class Solution:\n",
    "    def reconstructQueue(self, people: List[List[int]]) -> List[List[int]]:\n",
    "        people.sort(key=lambda x: (-x[0], x[1]))\n",
    "        ans = []\n",
    "        for person in people:\n",
    "            ans.insert(person[1], person)\n",
    "        return ans"
   ]
  }
