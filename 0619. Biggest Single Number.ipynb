   "source": [
    "import pandas as pd\n",
    "\n",
    "def biggest_single_number(my_numbers: pd.DataFrame) -> pd.DataFrame:\n",
    "    my_numbers.drop_duplicates(keep=False, inplace=True)\n",
    "    ans = my_numbers.max().max()\n",
    "    return pd.DataFrame({\"num\":[ans or pd.NA]})"
   ]
  }
