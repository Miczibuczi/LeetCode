   "source": [
    "class Solution:\n",
    "    def isSubsequence(self, s: str, t: str) -> bool:\n",
    "        for i in t:\n",
    "            if i == s[:1]:\n",
    "                s = s[1:]\n",
    "        return len(s) == 0"
   ]
  }
