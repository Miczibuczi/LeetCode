   "source": [
    "class Solution:\n",
    "    def chalkReplacer(self, chalk: List[int], k: int) -> int:\n",
    "        k %= sum(chalk)\n",
    "        for index, i in enumerate(chalk):\n",
    "            k -= i\n",
    "            if k < 0:\n",
    "                return index"
   ]
  }
