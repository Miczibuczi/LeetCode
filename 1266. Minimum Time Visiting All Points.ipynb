   "source": [
    "class Solution:\n",
    "    def minTimeToVisitAllPoints(self, points: List[List[int]]) -> int:\n",
    "        answer = 0\n",
    "        for i in range(1, len(points)):\n",
    "            answer += max(abs(points[i][0]-points[i-1][0]), abs(points[i][1]-points[i-1][1]))\n",
    "        return answer"
   ]
  }
