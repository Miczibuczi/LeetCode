   "source": [
    "class Solution:\n",
    "    def detectCapitalUse(self, word: str) -> bool:\n",
    "        if word == word.upper():\n",
    "            return True\n",
    "        elif word == word.lower():\n",
    "            return True\n",
    "        elif word == word.capitalize():\n",
    "            return True\n",
    "        return False"
   ]
  }
