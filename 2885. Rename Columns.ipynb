    "import pandas as pd\n",
    "\n",
    "def renameColumns(students: pd.DataFrame) -> pd.DataFrame:\n",
    "    students = students.rename(columns={\n",
    "        \"id\" : \"student_id\",\n",
    "        \"first\" : \"first_name\",\n",
    "        \"last\" : \"last_name\",\n",
    "        \"age\" : \"age_in_years\"\n",
    "    })\n",
    "    return students"
   ]
  }
