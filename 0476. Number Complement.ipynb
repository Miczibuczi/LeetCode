   "source": [
    "class Solution:\n",
    "    def findComplement(self, num: int) -> int:\n",
    "        x = bin(num)\n",
    "        y = \"0b\"\n",
    "        for i in range(2, len(x)):\n",
    "            if x[i] == \"0\":\n",
    "                y += \"1\"\n",
    "            else:\n",
    "                y += \"0\"\n",
    "        return int(y, 2)"
   ]
  }
