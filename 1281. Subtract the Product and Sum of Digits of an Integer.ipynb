   "source": [
    "class Solution:\n",
    "    def subtractProductAndSum(self, n: int) -> int:\n",
    "        s = 0\n",
    "        p = 1\n",
    "        while n > 0:\n",
    "            num = n % 10\n",
    "            s += num\n",
    "            p *= num\n",
    "            n //= 10\n",
    "        return p-s"
   ]
  }
