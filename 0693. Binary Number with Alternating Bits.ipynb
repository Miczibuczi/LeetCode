   "source": [
    "class Solution:\n",
    "    def hasAlternatingBits(self, n: int) -> bool:\n",
    "        n = bin(n)\n",
    "        for i in range(2, len(n)-1):\n",
    "            if n[i] == n[i+1]:\n",
    "                return False\n",
    "        return True"
   ]
  }
